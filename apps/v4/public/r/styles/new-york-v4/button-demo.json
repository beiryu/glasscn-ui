  "files": [
    {
      "path": "registry/new-york-v4/examples/button-demo.tsx",
      "content": "import {\n  ArrowLeftIcon,\n  ArrowRightIcon,\n  ArrowUpDownIcon,\n  BookmarkIcon,\n  CheckIcon,\n  ChevronDownIcon,\n  PencilIcon,\n  SearchIcon,\n  TrashIcon,\n} from \"lucide-react\"\n\nimport { Button } from \"@/registry/new-york-v4/ui/button\"\n\nexport default function ButtonDemo() {\n  return (\n    <div className=\"flex flex-col gap-4\">\n      {/* Top Row - Action Buttons */}\n      <div className=\"flex flex-wrap items-center gap-2\">\n        <Button variant=\"success\">\n          <CheckIcon />\n          Approve\n        </Button>\n        <Button variant=\"destructive\">\n          <TrashIcon />\n          Delete\n        </Button>\n        <Button>\n          <PencilIcon />\n          Edit\n        </Button>\n        <Button>\n          <SearchIcon />\n          Search\n        </Button>\n        <Button>\n          <BookmarkIcon />\n          Save\n        </Button>\n      </div>\n\n      {/* Middle Row - Navigation Buttons */}\n      <div className=\"flex flex-wrap items-center gap-2\">\n        <Button variant=\"secondary\">\n          <ArrowLeftIcon />\n          Previous\n        </Button>\n        <Button variant=\"secondary\">\n          <ArrowRightIcon />\n          Next\n        </Button>\n      </div>\n\n      {/* Bottom Row - Control Buttons */}\n      <div className=\"flex flex-wrap items-center gap-2\">\n        <Button variant=\"outline\">\n          Dropdown\n          <ChevronDownIcon />\n        </Button>\n        <Button variant=\"outline\">\n          Option\n          <ArrowUpDownIcon />\n        </Button>\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ],
  "type": "registry:example"
}